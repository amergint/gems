{
  "name": "gems",
  "version": "0.1.0",
  "description": "GEMS library",
  "main": "dist/index.js",
  "scripts": {
    "check-coverage": "nyc check-coverage --statements 100 --functions 100 --lines 100",
    "watch:test": "npm t -- -w",
    "test": "mocha src/index.test.js --compilers js:babel-register",
    "cover": "nyc npm t",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amergint/gems.git"
  },
  "keywords": [
    "GEMS",
    "OMG",
    "amergint",
    "AMERGINT"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Luis Rodriguez <luis@amergint.com> (http://www.amergint.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amergint/gems/issues"
  },
  "homepage": "https://github.com/amergint/gems#readme",
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-2": "6.13.0",
    "babel-register": "6.14.0",
    "chai": "3.5.0",
    "ghooks": "1.3.2",
    "istanbul": "0.4.5",
    "mocha": "3.0.2",
    "nyc": "8.1.0",
    "rimraf": "2.5.4"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
